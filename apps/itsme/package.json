		"@langchain/langgraph": "^0.2.49",
		"@langchain/openai": "^0.4.4",
		"@supabase/supabase-js": "^2.49.1",
		"@tanstack/react-query": "^5.69.0",
		"@tanstack/react-query-devtools": "^5.70.0",
		"@trpc/client": "^11.0.0",
		"@trpc/server": "^11.0.0",
		"@trpc/tanstack-react-query": "^11.0.0",
		"ai": "^4.1.54",
