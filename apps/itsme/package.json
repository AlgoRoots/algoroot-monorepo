{
	"name": "@algoroot/itsme",
	"version": "1.2.3",
	"type": "module",
	"scripts": {
		"dev": "next dev --turbopack --port 3000",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"analyze": "ANALYZE=true pnpm build",
		"check-types": "tsc --noEmit",
		"update-qa": "curl -X POST http://localhost:3000/api/questions",
