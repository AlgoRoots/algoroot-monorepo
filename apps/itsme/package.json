		"@tailwindcss/postcss": "^4.0.9",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22",
		"@types/react": "19.0.8",
		"@types/react-dom": "19.0.3",
		"@types/react-syntax-highlighter": "^15.5.13",
		"dotenv": "^16.4.7",
		"node-html-markdown": "^1.3.0",
