		"tw:watch": "tailwindcss -i ./src/styles/global.css -o ./dist/output.css --watch",
		"gen:db-types": "dotenv -e .env -- sh -c 'npx supabase gen types typescript --project-id \"$PROJECT_REF\" --schema public > ./src/modules/api/supabase/database.types.ts'"
	},
