{
	"name": "@algoroot/itsme",
	"version": "1.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack --port 3000",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"check-types": "tsc --noEmit",
		"add-qa": "ENV_FILE=.env npx tsx app/actions/create-questions.ts",
		"test-pdf": "ENV_FILE=.env npx tsx modules/vector-store/utils/get-docs-pdf.ts",
		"test-md": "ENV_FILE=.env npx tsx modules/vector-store/utils/get-docs-md.ts",
		"update-qa": "curl -X POST http://localhost:3000/api/questions",
		"tw:output": "tailwindcss -i ./src/styles/global.css -o ./dist/output.css",
		"tw:watch": "tailwindcss -i ./src/styles/global.css -o ./dist/output.css --watch"
	},
	"dependencies": {
		"@algoroot/shared": "workspace:*",
		"@algoroot/ui": "workspace:*",
