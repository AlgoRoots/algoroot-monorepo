		"@langchain/core": "^0.3.40",
		"@langchain/langgraph": "^0.2.49",
		"@langchain/openai": "^0.4.4",
		"@supabase/supabase-js": "^2.49.1",
		"ai": "^4.1.54",
		"langchain": "^0.3.19",
		"lodash-es": "^4.17.21",
		"lucide-react": "^0.475.0",
		"next": "^15.1.6",
		"next-themes": "^0.4.4",
		"pnpm": "^10.5.1",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-markdown": "^10.1.0",
		"react-syntax-highlighter": "^15.6.1",
